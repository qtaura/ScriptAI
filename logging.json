{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "json": {
      "()": "monitoring.JSONFormatter"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "json",
      "stream": "ext://sys.stdout"
    },
    "file": {
      "class": "logging.FileHandler",
      "level": "INFO",
      "formatter": "json",
      "filename": "scriptai.log",
      "encoding": "utf-8"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": ["console", "file"]
  },
  "loggers": {
    "ScriptAI": {
      "level": "INFO",
      "propagate": true
    },
    "ScriptAI.Security": {
      "level": "INFO",
      "propagate": true
    }
  }
}